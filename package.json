{
    "name": "gold",
    "description": "Classy black&gold",
    "version": "0.1.0",
    "engines": {
        "ghost-api": "v3"
    },
    "nodemonConfig": {
        "watch": [
            "sass/**/*"
        ],
        "ext": [
            "sass",
            "scss"
        ],
        "delay": "1500",
        "events": {
            "restart": ""
        }
    },
    "license": "MIT",
    "author": {
        "name": "Dariusz Damian Jakubowski",
        "email": "contact@celber.pl"
    },
    "config": {
        "posts_per_page": 10
    },
    "devDependencies": {
        "node-sass": "^7.0.0",
        "nodemon": "^1.19.4"
    },
    "dependencies": {
        "json-format": "^1.0.1"
    }
}
