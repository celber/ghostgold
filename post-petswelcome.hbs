{{#contentFor "header"}}
    <header>
        <div class="header-inner">
            <div class="description">
                {{@site.description}}
            </div>
            <div class="name">
                {{@site.title}}
            </div>
            <div class="page">
                BLOG
            </div>
            <div class="separator">

            </div>
        </div>
    </header>
    <style>
        header {
            background: transparent url({{@site.cover_image}}) center bottom/cover repeat scroll;
        }

nav.topbar {
z-index: 1999;
}

        .category-toggle {
            display: inline-block;
            padding: 17px;
            padding-left: 45px;
            margin: 5px;
            border-radius: 40px;
            opacity: .5;
            background-color: rgb(45, 46, 44);
            position: relative;
        }

        .category-toggle.enabled {
            opacity: 1;
        }

        .category-toggle::before {
            left: 20px;
            background-color: rgb(27, 27, 27);
            content: " ";
            border-radius: 20px;
            position: absolute;
            display: block;
            width: 10px;
            height: 10px;
            top: 22px;
        }

        .category-toggle.enabled::before {
            background-color: rgb(61, 194, 27);
        }

        #places {
            display: flex;
            flex-direction: row;
            flex-wrap: wrap;
            justify-content: center;
            align-items: flex-start;
            align-content: flex-start;
            text-align: left;
            padding: 0;
        }

        .place {
            width: 28%;
            height: 80px;
            border-radius: 8px;
            padding: 12px;
            margin: 5px;
            background-color: rgb(45, 46, 44);
        }

        @media (max-width: 768px) {
            .place {
                width: 40%;
            }
        }
        @media (max-width: 576px) {
            .place {
                width: 90%;
            }
        }



        .place .category {
            color: var(--gold);
            text-transform: uppercase;
            font-size: .9rem;
        }

        .place .name {
            font-weight: 600;
            font-size: 1.2rem;
        }

        .place .address {
            font-size: 0.7rem;
        }

        .place.recommended {
            background-color: var(--gold);

            color: black;
        }

        .place.recommended .category {
            color: white;
        }

        .hidden {display: none;}
    </style>
{{/contentFor}}

{{#contentFor "content"}}
            <article class="portfolio {{post_class}}">
                <div class="content">

						{{#post}}

            			<h1>{{title}}</h1>
						{{content}}

    					{{/post}}
						<br><br>
                        <div class='category-toggle enabled' data-filter='restaurant'>Restauracje</div>
                        <div class='category-toggle enabled' data-filter='cafe'>Kawiarnie</div>
                        <div class='category-toggle enabled' data-filter='pub'>Bary</div>
                        <div class='category-toggle enabled' data-filter='hotel'>Hotele</div>
                        <div class='category-toggle enabled' data-filter='shop'>Sklepy</div>
                        <div class='category-toggle enabled' data-filter='other'>Inne</div>

                        <br><br>
                        <div id="mapid" style="height: calc(100vh - 150px); width: 100%"></div>

