    <script type="application/javascript">
        var Wikimedia = L.tileLayer('https://maps.wikimedia.org/osm-intl/{z}/{x}/{y}{r}.png', {
            attribution: '<a href="https://wikimediafoundation.org/wiki/Maps_Terms_of_Use">Wikimedia</a>',
            minZoom: 1,
            maxZoom: 19
        });
        var mymap = L.map('mapid', {
            gestureHandling: true
        }).setView([51.107883, 17.038538], 13);
        Wikimedia.addTo(mymap);

        var $placesEl = $('#places');
        var $placeBtn = null;

        var restaurantIcon = L.icon({
            iconUrl: "{{asset '/images/marker-restaurant.png'}}",
            iconSize: [38, 50],
            iconAnchor: [19, 50],
            popupAnchor: [0, -50]
        });

        var pubIcon = L.icon({
            iconUrl: "{{asset '/images/marker-pub.png'}}",
            iconSize: [38, 50],
            iconAnchor: [19, 50],
            popupAnchor: [0, -50]
        });

        var cafeIcon = L.icon({
            iconUrl: "{{asset '/images/marker-cafe.png'}}",
            iconSize: [38, 50],
            iconAnchor: [19, 50],
            popupAnchor: [0, -50]
        });

        var hotelIcon = L.icon({
            iconUrl: "{{asset '/images/marker-hotel.png'}}",
            iconSize: [38, 50],
            iconAnchor: [19, 50],
            popupAnchor: [0, -50]
        });

        var shopIcon = L.icon({
            iconUrl: "{{asset '/images/marker-shop.png'}}",
            iconSize: [38, 50],
            iconAnchor: [19, 50],
            popupAnchor: [0, -50]
        });

        var otherIcon = L.icon({
            iconUrl: "{{asset '/images/marker-other.png'}}",
            iconSize: [38, 50],
            iconAnchor: [19, 50],
            popupAnchor: [0, -50]
        });

        var categoryNames = {
            pub: "Bar",
            restaurant: "Restauracja",
            cafe: "Kawiarnia",
            shop: "Sklep",
            hotel: "Hotel",
            other: 'Inne'
        };

        var categoryIcon = null;
        var categories;

        window.places.forEach(place => {
            place.categories = place.category.split(', ');

            categories = place.categories.map(cat => categoryNames[cat]);


            $placeBtn = $(`<liv class="place">

                    <span class="category">${categories.join(', ')}</span>
                    <br>
                    <span class="name">${place.name}</span>
                    <br>
                    <span class="address">${place.address}</span>
                    </li>`);
                    
            $placesEl.append($placeBtn);

            if (place.recommended) {
                $placeBtn.addClass('recommended')
            }

            if (place.categories[0] == 'restaurant') {
                categoryIcon = restaurantIcon;
            } else if (place.categories[0] == 'pub') {
                categoryIcon = pubIcon;
            } else if (place.categories[0] == 'hotel') {
                categoryIcon = hotelIcon;
            } else if (place.categories[0] == 'shop') {
                categoryIcon = shopIcon;
            } else if (place.categories[0] == 'cafe') {
                categoryIcon = cafeIcon;
            } else {
                categoryIcon = otherIcon;
            }

            place.marker = L.marker([place.location.lat, place.location.lng],{
                icon: categoryIcon
            }).addTo(mymap);

            place.el = $placeBtn;
            place.visible = true;

            place.popup = place.marker.bindPopup(place.name)


            $placeBtn.on('click',function (evt) {
                window.scrollTo(0, document.getElementById('mapid').offsetTop - 100);
                mymap.setView(place.marker.getLatLng(),17)
                place.popup.openPopup();
            });
        });

        function dispatchFilters() {
            var filters = $('.category-toggle.enabled').map(function () {
                return $(this).data('filter')
            });

            filters = Array.prototype.slice.call( filters, 0 );
            var toBeEnabled = [];
            var toBeDisabled = [];

            var enable = false;
            window.places.forEach(function (place) {
                enable = place.categories.some(function (category) {
                    if (filters.includes(category)) {
                        return true;
                    }
                });

                if (enable && !place.visible) {
                    place.marker.addTo(mymap);
                    toBeEnabled.push(place);
                } else if (!enable && place.visible) {
                    place.marker.remove();
                    toBeDisabled.push(place);
                }
            });

            toBeDisabled.forEach(place => {
                place.el.hide();
                place.visible = false;
            });
            toBeEnabled.forEach(place => {
                place.el.show();
                place.visible = true;
            })
        }

        $('.category-toggle').on('click', function (evt) {
            $(this).toggleClass('enabled');
            dispatchFilters();
        });
    </script>
